<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cs4531.finalsoftware">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.sasha.finalsoftware.LikedNames"
            android:label="Liked/Disliked Names"></activity>
        <activity android:name="com.example.sasha.finalsoftware.NameGameFront"
            android:label="Choose What Names"/>
        <activity android:name="com.example.sasha.finalsoftware.FrontPage"
            android:label="Welcome To Baby Name Generator"/>
        <activity
            android:name="com.example.sasha.finalsoftware.MainActivity"
            android:label="Baby Name Tinder">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.sasha.finalsoftware.NamesListedActivity"
            android:label="Names" />
    </application>

</manifest>